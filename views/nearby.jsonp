SimpleGeo.response({
	'image': '<img class="map-static" id="static-map-img" src="http://maps.google.com/maps/api/staticmap?size={{config.map_dimensions}}&amp;format=jpeg&amp;sensor=false&amp;zoom=13&amp;{{markers}}">'
	,'nearby': '<ol class="parks">{% for park in nearby %}<li data-guid="{{park.guid}}"{% if forloop.first %} class="first-child"{% endif %}><a href="/parks/{{park.guid}}" class="parkinfo" role="button"><span class="rategroup"><span class="value">{{park.distance|floatformat:1}}</span><span class="symbol">{{l.distance_km}}</span></span><strong class="park-info"><span class="name">{{park.name}}</span> <small class="amount-votes"><span class="vote-total">{{park.amountVotes}}</span> {% ifequal park.amountVotes 1 %}{{l.rating}}{% else %}{{l.ratings}}{% endifequal %}</small></strong>{% if not park.dogs %}<em class="park-type">{{l.dogs_no}}</em>{% else %}{% if not park.restrictions %}{% if not park.leashed %}<em class="park-type free">{{l.dogs_free}}</em>{% else %}<em class="park-type leashed">{{l.dogs_leashed}}</em>{% endif %}{% else %}<em class="park-type restrictions">{{l.dogs_restricted}}</em>{% endif %}{% endif %}</a><a href="#rate" class="rate" role="button">{{l.rate}}</a></li>{% endfor %}</ol>'
});
